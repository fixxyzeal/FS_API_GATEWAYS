{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Services": {
    "dbgateway": {
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "dbgatewayapi.herokuapp.com",
          "Port": 443
        }
      ]
    },
    "notification": {
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "notificationserviceapi.herokuapp.com",
          "Port": 443
        }
      ]
    }
  }
}